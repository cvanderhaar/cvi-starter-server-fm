{
	"name": "iota-starter-server-fleetmanagement-webclient",
	"version": "0.0.1",
	"private": true,
	"engines": {
		"npm": "2.*",
		"node": "8.*"
	},
	"scripts": {
		"start": "tsc && gulp && concurrently \"npm run tsc:w\" \"npm run lite\"",
		"lite": "lite-server -c ./bs-config.json",
		"postinstall": "npm ls typings && typings install || echo .",
		"tsc": "tsc",
		"tsc:w": "tsc -w",
		"typings": "typings",
		"gulp": "gulp"
	},
	"dependencies": {
		"@angular/common": "7.2.15",
		"@angular/compiler": "7.2.15",
		"@angular/core": "7.2.15",
		"@angular/forms": "7.2.15",
		"@angular/http": "7.2.15",
		"@angular/platform-browser": "7.2.15",
		"@angular/platform-browser-dynamic": "7.2.15",
		"@angular/router": "7.2.15",
		"@angular/upgrade": "7.2.15",
		"systemjs": "0.21.5",
		"core-js": "^2.6.9",
		"reflect-metadata": "^0.1.3",
		"rxjs": "6.5.2",
		"zone.js": "^0.9.1"
	},
	"devDependencies": {
		"concurrently": "^4.1.0",
		"lite-server": "^2.5.4",
		"typescript": "^3.5.1",
		"typings": "^2.1.1",
		"systemjs-builder": "^0.15.17",
		"gulp": "^4.0.2",
		"gulp-rename": "^1.4.0",
		"gulp-clean-css": "^4.2.0",
		"gulp-concat-css": "^3.1.0",
		"rimraf": "^2.6.3"
	}
}
